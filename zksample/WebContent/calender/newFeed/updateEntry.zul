<zk>
	<window id="updateMyEntry" title="Update Entry" border="normal" mode="popup" apply="com.calender.newsfeed.controllers.EventUpdateController" visible="false">
		<grid>
			<rows>
				<row>
					Item: <textbox id="tbText" cols="40" value="" />
				</row>
				<row>
					Type: <combobox id="cmbType">
						<comboitem label="Release" />
						<comboitem label="Small talk" />
						<comboitem label="Event" />
					  </combobox>
				</row>
				<row>
					Submit:
					<button id="btnUpdateNews" label="Update" width="36px" height="24px"/>
					<button id="btnDeleteNews" label="Delete" width="36px" height="24px"/>
					<button id="btnCancel" label="Cancel" width="36px" height="24px" />
				</row>	
			</rows>
		</grid>
	</window>
</zk>